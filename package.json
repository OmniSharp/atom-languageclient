  },
  "author": "OmniSharp Team",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/OmniSharp/atom-languageclient/issues"
  },
  "homepage": "https://github.com/OmniSharp/atom-languageclient#readme",
  "dependencies": {
    "atom-languageservices": "~1.0.0",
    "atom-package-deps": "^4.3.1",
    "aurelia-dependency-injection": "^1.3.0",
    "chokidar": "^1.6.1",
    "file-url": "^1.1.0",
    "fuse.js": "^2.6.1",
    "lodash": "^4.17.4",
    "reflect-metadata": "^0.1.9",
    "rxjs": "^5.0.3",
    "semver": "^5.3.0",
    "symbol-observable": "^1.0.2",
    "ts-disposables": "^2.2.3",
    "vscode-jsonrpc": "^3.0.3",
    "vscode-languageserver-types": "^3.0.3"
  },
  "devDependencies": {
    "@types/chai": "^3.4.34",
    "@types/chokidar": "^1.4.31",
    "@types/event-kit": "^1.2.30",
    "@types/mocha": "^2.2.38",
    "@types/node": "^7.0.4",
    "@types/rimraf": "0.0.28",
    "@types/semver": "^5.3.30",
    "chai": "^3.5.0",
    "codelyzer": "0.0.26",
    "globby": "^6.1.0",
    "mocha": "^3.2.0",
    "rimraf": "^2.5.4",
    "tslint": "^4.4.1",
    "tslint-microsoft-contrib": "^4.0.0",
    "typescript": "^2.1.5",
    "typings": "^2.1.0"
  },
  "atomTestRunner": "./spec/test-runner",
  "package-deps": [
    "linter"
