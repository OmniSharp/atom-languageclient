{
    "compilerOptions": {
        "target": "es5",
        "lib": [
            "es5",
            "dom",
            "es2015.core",
            "es2015.collection",
            "es2015.generator",
            "es2015.iterable",
            "es2015.promise",
            "es2015.reflect",
            "es2015.symbol",
            "es2015.symbol.wellknown"
        ],
        "module": "commonjs",
        "moduleResolution": "node",
        "noEmitOnError": false,
        "declaration": false,
        "noImplicitAny": true,
        "preserveConstEnums": true,
        "skipLibCheck": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "allowSyntheticDefaultImports": true,
        "allowUnreachableCode": false,
        "sourceMap": true
    },
    "exclude": [
    ]
}
